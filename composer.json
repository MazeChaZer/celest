{
  "name": "celest/celest",
  "description": "A port of JSR310 to PHP",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Steffen Hanikel",
      "email": "steffen.hanikel@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Celest\\": "src/Celest"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Celest\\": "test/Celest"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php" : ">=5.6",
    "celest/tzdata" : "~0.1",
    "php-64bit": "*",
    "ext-gmp": "*",
    "ext-intl": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
